{
  "expo": {
    "name": "OSLEventApp",
    "slug": "OSLEventApp",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "scheme": "vike",
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-google-signin/google-signin",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "expo-router"
    ],
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "edu.augustana.osleventapp"
    },
    "ios": {
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "edu.augustana.osleventapp"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "aad3b70c-0cc7-42bd-bf30-2cf5b319608f"
      }
    }
  }
}
